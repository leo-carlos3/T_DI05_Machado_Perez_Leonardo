	<query language="SQL"><![CDATA[SELECT artists. ArtistId ,
	artists. Name ,
	albums. Title ,
	albums. AlbumId ,
	albums. ArtistId ,
	tracks. Name ,
	tracks. AlbumId ,
	tracks. Composer ,
	tracks. UnitPrice ,
	tracks. Bytes 
FROM tracks
	INNER JOIN albums ON 
	 tracks. AlbumId  = albums. AlbumId  
	INNER JOIN artists ON 
	 albums. ArtistId  = artists. ArtistId
	 WHERE artists. ArtistId=$P{id}]]></query>
