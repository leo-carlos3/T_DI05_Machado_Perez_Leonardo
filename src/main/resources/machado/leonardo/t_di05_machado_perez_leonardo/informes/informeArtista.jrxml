	<query language="SQL"><![CDATA[SELECT artists. Name ,
	artists. ArtistId ,
	albums. Title ,
	tracks. Name ,
	tracks. GenreId ,
	tracks. Composer ,
	tracks. Milliseconds ,
	tracks. UnitPrice 
FROM tracks
	INNER JOIN albums ON 
	 tracks. AlbumId  = albums. AlbumId  
	INNER JOIN artists ON 
	 albums. ArtistId  = artists. ArtistId
WHERE artists. ArtistId=$P{ID}
]]></query>
